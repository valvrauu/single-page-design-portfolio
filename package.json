{
  "name": "single-page-design-portfolio",
  "version": "1.0.0",
  "description": "Frontend Mentor - Single-page design portfolio",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass ./src/styles/sass/main.scss ./public/css/min.css",
    "prefix:css": "postcss ./public/css/min.css --use autoprefixer -o ./public/css/min.css",
    "compress:css": "node-sass ./public/css/min.css ./public/css/min.css --output-style compressed",
    "watch:css": "node-sass ./src/styles/sass/main.scss ./public/css/min.css -w",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "keywords": [],
  "author": "Valvrauu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concat": "^1.0.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0"
  }
}
